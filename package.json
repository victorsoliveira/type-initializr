{
    "name": "type-initializr",
    "version": "1.0.2",
    "description": "Uma ferramenta para instanciar classes existentes baseasas em modelos com valores padr√µes",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "test": "jest --config jest.json",
        "build": "tsc",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
        "lint": "tslint -p tsconfig.json",
        "prepare": "npm run build",
        "prepublishOnly": "npm test && npm run lint",
        "preversion": "npm run lint",
        "version": "npm run format && git add -A src",
        "postversion": "git push && git push --tags"
    },
    "files": [
        "lib/**/*"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/victorsoliveira/type-initializr.git"
    },
    "keywords": [
        "Type",
        "Initializer",
        "Instantiate"
    ],
    "author": "Victor Oliveira <contato@victoroliveira.dev>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/victorsoliveira/type-initializr/issues"
    },
    "homepage": "https://github.com/victorsoliveira/type-initializr#readme",
    "devDependencies": {
        "@types/jest": "^25.1.4",
        "jest": "^25.1.0",
        "prettier": "^1.19.1",
        "ts-jest": "^25.2.1",
        "tslint": "^6.1.0",
        "typescript": "^3.8.3"
    },
    "dependencies": {
        "reflect-metadata": "^0.1.13"
    }
}
